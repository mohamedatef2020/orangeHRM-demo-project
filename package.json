{
  "name": "test-automation-demo-project-on-orangehrm",
  "version": "1.0.0",
  "description": "Test automation demo project using Cypress & javascript on orang HRM demo website",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "open-cypress": "npx cypress open",
    "generate-report": "node ./generateHTMLReport.js",
    "open-report": "npx open-cli report/HTML/cucumber_report.html",
    "test-and-report": "npm run test && npm run generate-report && npm run open-report"
  },
  "keywords": [],
  "author": {
    "name": "Mohamed Atef",
    "email": "mohamed.atef.pd2020@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "cypress": "^13.11.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "open-cli": "^8.0.0",
    "rimraf": "^5.0.7",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cucumber/cucumber": "latest",
    "@types/cypress-cucumber-preprocessor": "latest",
    "cucumber-html-reporter": "^7.1.1",
    "@badeball/cypress-cucumber-preprocessor": "latest"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/**/*.{js,ts}",
    "json": {
      "enabled": true,
      "output": "report/JSON/cucumber-report.json"
    },
    "repository": {},
    "filterSpecs": true,
    "omitFiltered": true
  }
}
